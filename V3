local function glmchns(n)
local a,r="abcdefghijklmnopqrstuvwxyz","" local b=a.."0123456789_"
r=a:sub(math.random(1,#a),math.random(1,#a))
for i=2,(n or math.random(5,9)) do local x=math.random(1,#b) r=r..b:sub(x,x) end return r end
